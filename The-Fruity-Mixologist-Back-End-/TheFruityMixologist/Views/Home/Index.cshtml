    <section id="favoriteCock">
        <div class="container">
            <div class="row">
                @if (highestRatedRecipe != null)
                {
                    <div class="col-lg-7 image">
                        <img style="width: 100%" src="~/assets/photos/Recipes/@highestRatedRecipe.RecipesImages.FirstOrDefault(ri => ri.IsMain == true).Path" />
                    </div>
                    <div class="col-lg-5 text">
                        <div class="favorit">Favorite Cocktail</div>
                        <div class="title">@highestRatedRecipe.Name</div>
                        <p class="branded">@highestRatedRecipe.RecipesCategories.First().Category.Name</p>
                        <div class="desc">@highestRatedRecipe.Desc</div>
                        <div class="price">$@highestRatedRecipe.Price</div>
                        <div class="buttons">
                            <a asp-controller="Shop" asp-action="Detail" asp-route-id="@highestRatedRecipe.Id">
                                <button class="addtobasket" type="submit">Add to basket</button>
                           </a>
                            <button class="seeallSets" type="submit"><a asp-controller="Shop" asp-action="Index">See all recipes</a></button>
                        </div>
                    </div>
                }
              
            </div>
        </div>
      </section>
      <section id="Reviews">
        <div class="container">
            <div class="row">
    <div class="title">
                    Reviews
                </div>
                <div class="text">
                    <span>There are many variations of passages of Lorem Ipsum available,
                        but the majority have suffered alteration in some form, by injected humour, or randomised.</span>
                </div>
               <div class="">
                <div class="comments center">
                    @foreach(var com in Model.comments)
                    {
                        <div class="comment">
                      <div class="text-comment">
                          <span>@com.Text</span>
                      </div>
                      <div class="user-profil">
                          <div class="user-photo">
                             <div class="image">
                                            <img style="width:60px; height:60px" src="~/assets/photos/myAccount/@com.User.Path">
                             </div> 
                                  <span class="user-name">@com.User.UserName</span>
                          </div>

                                    <div class="written-date">@com.CreationTime.ToString("MMM.dd, yyyy")</div>
                    </div>
  
                      </div>
                    }
                  </div>
               </div>
            </div>
        </div>
            
            
      </section>

      <section id="Partners">
        <div class="container">
            <div class="row">
                <div class="col-lg-3">
                    <div class="text">
                        Partners
                    </div>
                </div>
                <div class="col-lg-9">
                   <div class="partnersImg responsivePartner">

                        @await Html.PartialAsync("_partnerPartial",settings)

                   </div>
                </div>
            </div>
        </div>
      </section>


      <section id="Subscribe">
        <div class="title">
            Subscribe to Mailing List
        </div>
        <div class="text">
           <span> If you are going to use a passage of Lorem Ipsum, you need to be sure there isn't anything embarrassing hidden in the middle of text.</span>
        </div>
        <div class="sendEmailInputButton">
            <form asp-controller="Home" asp-action="Subscribe">
                <input  name="email" placeholder="Email address" >
            <button type="submit">Sign up</button>
          </form>
        </div>
      </section>
      
    </main>

