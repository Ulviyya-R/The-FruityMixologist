                    <div class="event-name">
                      <span>@item.Name</span>
                    </div>
                  </div>
                 
            }
                </div>
              </div>
            </div>
          </div>
        </div>
      </section>

      <section id="TrendingRecipes">
        <div class="container">
          <div class="row">
            <div class="title">
              <span>Trending recipes</span>
            </div>
            <div class="categorys">
              <ul>
                                                <li><a asp-controller="Home" asp-action="Sorts" name="sort" value="All">All</a></li>
                                                <li><a asp-controller="Home" asp-action="Sorts" name="sort" value="Alcoholic">Alcoholic</a></li>
                                                <li><a asp-controller="Home" asp-action="Sorts" name="sort" value="Non-Alcoholic">Non-Alcoholic</a></li>
              </ul>

                  <script>
                                                                const sortLinks = document.querySelectorAll('a[name="sort"]');

                                                sortLinks.forEach(link => {
                                                    link.addEventListener('click', (event) => {
                                                        event.preventDefault();
                                                        const selectedSort = link.getAttribute('value');

                                                        fetch(`/Home/Sorts?sort=${selectedSort}`, {
                                                            method: 'POST'
                                                        })
                                                            .then(response => response.text())
                                                            .then(result => {
                                                                document.getElementById("recipesResult").innerHTML = result;
                                                            })
                                                            .catch(error => console.log(error));
                                                    });
                                                });
            </script>
            </div>
                <div class="products" id="recipesResult">

							@await Html.PartialAsync("_HomeRecipePartial",recipes)


          </div>
        </div>
      </section>

