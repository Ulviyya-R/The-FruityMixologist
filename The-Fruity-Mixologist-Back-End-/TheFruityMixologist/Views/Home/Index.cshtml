@model SlidersVM
@{
    ViewData["Title"] = "Index";
    Layout = "_Layout";
    List<Recipe> recipes = ViewBag.HomeRecipes;
    List<Recipe> raytingRecipes = ViewBag.RaytingRecipes as List<Recipe>;
    Recipe highestRatedRecipe = raytingRecipes.Where(r => r.PointReyting > 3).OrderByDescending(r => r.PointReyting).FirstOrDefault();
    Subscribe subscribe = ViewBag.Subscribe;
    List<Setting> settings = ViewBag.PartnerPartial;
}

<main>
      <section id="CreateOwnCocktail">
        <div class="container">
          <div class="row">
            <div class="col-lg-6 image">
              
            </div>
            <div class="col-lg-6 text">
              <div class="title">
                <h1>
                  Create your own cocktail experience with our customizable page
                </h1>
              </div>
              <div class="descAndButtons">
                <div class="desc">
                  <p>
                    Choose your liquor, mixers, and garnishes to craft your
                    perfect sip. Elevate your happy hour with a drink that's truly
                    yours. Let's cheers to your individuality!
                  </p>
                 
                </div>
                <div class="button">
                  <div class="icon">
                    <svg
                      width="15"
                      height="14"
                      viewBox="0 0 15 14"
                      fill="none"
                      xmlns="http://www.w3.org/2000/svg"
                    >
                      <path
                        d="M1 7H13.5M13.5 7L7.5 1M13.5 7L7.5 13"
                        stroke="#ED0039"
                        stroke-width="1.5"
                        stroke-linecap="round"
                        stroke-linejoin="round"
                      />
                    </svg>
                  </div>
                            <a asp-controller="CreateCocktail" asp-action="Index">Create your own cocktail</a>
                </div>
              <!-- <div class="lean"></div> -->
           
            </div>
          </div>
        </div>
      </section>
      <section id="PopularEvents">
        <div class="container">
          <div class="row">
            <div class="col-lg-5 text">
              <span>Popular Events</span>

              
              <p>
                There are many variations of passages of Lorem Ipsum available,
                but the majority have suffered alteration in some form, by
                injected humour, or randomised words which don't look even
                slightly believable.If you are going to use a passage of Lorem
                Ipsum, you need to be sure there isn't anything embarrassing
                hidden in the middle of text.
              </p>
            </div>
            <div class="col-lg-7">
              <div class="">
                <div class="slider-images responsive">
              @foreach(var item in Model.slider){
                  <div class="event">
                    <div class="image">
                     <img style="width: 100%; height:100%"
                 src="~/assets/photos/Home/Slider/@item.ImagePath" />
                    </div>
                    <div class="date">
                      <span class="dates">@item.Date.ToString("MMM dd, yyyy")</span>
                      <div class="line"></div>
                      <span class="clock">
                        <svg
                          width="24"
                          height="24"
                          viewBox="0 0 24 24"
                          fill="none"
                          xmlns="http://www.w3.org/2000/svg"
                        >
